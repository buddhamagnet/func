<h4>TOPICS</h4>
  <ul class="collection">
    <%= for topic <- @topics do %>
      <li class="collection-item">
        <%= topic.title %>
        <div class="right">
          <%= link "edit", to: topic_path(@conn, :edit, topic) %>
        </div>
      </li>
    <%= end %>
  </ul>
  <div class="fixed-action-btn">
    <%= link class: "btn-floating btn-large waves-effect waves-light red", to: topic_path(@conn, :new) do %>
      <i class="material-icons">add</i>
    <% end %>
  </div>