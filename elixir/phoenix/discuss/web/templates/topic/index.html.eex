<h4>TOPICS</h4>
  <ul class="collection">
    <%= for topic <- @topics do %>
      <li class="collection-item">
        <%= link topic.title, to: topic_path(@conn, :edit, topic) %>

      </li>
    <%= end %>
  </ul>
  <div class="fixed-action-btn">
    <%= link class: "btn-floating btn-large waves-effect waves-light red", to: topic_path(@conn, :new) do %>
      <i class="material-icons">add</i>
    <% end %>
  </div>